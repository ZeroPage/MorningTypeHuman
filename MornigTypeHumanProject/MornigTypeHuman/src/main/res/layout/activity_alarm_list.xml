<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".view.AlarmListActivity">

    <org.zeropage.gdg.morningTypeHuman.view.alarmlist.AlarmAddButton
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="알람 추가하기"
        android:id="@+id/buttonAddAlarm" />

    <org.zeropage.gdg.morningTypeHuman.view.alarmlist.AlarmListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView" />

</LinearLayout>
